<div class="col-md-4 text-center">
  <div class="row">
    <h2 class="intro-text"><%= title.capitalize %></h2>
    <% array.each do |vendor| %>
      <div class="col-md-12">
        <div class="text-center">
          <p style="background-color:#FDFDFD"> <%= link_to vendor.name, vendor_show_path(vendor.slug) %>
         </p>
        </div>
      </div>
  <% end %>
  </div>
</div>
